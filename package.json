{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build && npx prisma migrate deploy && npx prisma generate",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "remix": "remix dev",
    "build:css": "tailwindcss -o ./app/styles/tailwind.css --minify",
    "dev:css": "tailwindcss -o ./app/styles/tailwind.css --watch",
    "start": "remix-serve build",
    "css": "tailwindcss -w -i app/styles/input.css -o app/styles/tailwind.css --watch",
    "migrate": "npx prisma migrate dev --name",
    "format": "npx prisma format",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "eslint ./app --ext .js,.jsx,.ts,.tsx",
    "format:lint": "npm run lint -- --fix && prettier --write ./app"
  },
  "dependencies": {
